<?xml version="1.0" encoding="UTF-8"?>
<document>
	<simulation log="EXPRESSIVE" outputfolder="../results" name="pH_0D_phosphoricAcid">
		<timer stepSize="3 [h]" endOfSimulation="10 [d]" />
		<speciesLib>
		</speciesLib>
		<compartment name="chemostat">
			<shape class="Dimensionless" volume="1 [L]">
				<boundary  class="constantConcentrationToChemostat" volumeFlowRate="0.1 [L/h]">
				<solute name="phosphoricAcid" concentration="0.1 [mmol/L]"  />
				</boundary>
			</shape>
			<solutes>
				<solute name="phosphoricAcid" pKa="2.15,7.2,12.35" concentration="0.1 [mmol/L]" />
			</solutes>
			<reactions>
			</reactions>
			<agents>
			</agents>
			<processManagers>
				<process name="solveChemostat" class="ChemostatSolver" priority="0"
					firstStep="0.0" >
					<aspect name="hMax" type="PRIMARY" class="Double" value="0.05" />
					<aspect name="solver" type="PRIMARY" class="String" value="heun" />
					<aspect name="tolerance" class="Double" value="1.0E-9" />
				</process>
			</processManagers>
		</compartment>
	</simulation>
</document>